            language = <[ISO_639-1::en]>
            purpose = <"A simple test archetype for parser development.">
            keywords = <"test", "example", "simple">
        >
    >

definition
    OBSERVATION[id1] matches {
        data matches {
            HISTORY[id2] matches {
                events cardinality matches {1..*} matches {
                    EVENT[id3] occurrences matches {1..*} matches {
                        data matches {
                            ITEM_TREE[id4] matches {
                                items cardinality matches {0..*} matches {
                                    ELEMENT[id5] occurrences matches {0..1} matches {
                                        value matches {
                                            DV_TEXT[id6]
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }

